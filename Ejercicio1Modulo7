import Dos as d
import pprint
def main( a, b):
    pprint.pprint(d.suma(a, b))
    pprint.pprint(d.resta(a, b))
    pprint.pprint(d.multiplicacion(a, b))
    pprint.pprint(d.division(a, b))



if __name__ == '__main__':
    main(2, 4)
---------------------------------------------
__init__.py
---------------------------------------------
def suma(a, b):
    return a + b


def resta(a, b):
    return a - b

def multiplicacion(a, b):
    return a * b

def division(a, b):
    return a / b
