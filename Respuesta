class Alumno:
    def __init__(self, nombre, nota):
        self.nombre = nombre
        self.nota = nota

    def mostrar(self):
        print(f"Nombre: {self.nombre}")
        print(f"Nota: {self.nota}")

    def resultado(self):
        if self.nota > 12:
            print(f"{self.nombre} --> APROBO")
        else:
            print(f"{self.nombre} --> REPROBO")

a = Alumno("Yossef", 10)
a.mostrar()
a.resultado()
b = Alumno("Zidane", 18)
b.mostrar()
b.resultado()
